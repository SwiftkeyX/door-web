generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Log {
    id       Int      @id @default(autoincrement())
    time     DateTime @default(now())
    pet_name String
    message  String
}

model Pet {
    id       Int    @id @default(autoincrement())
    uid      String @unique
    pet_name String
}
